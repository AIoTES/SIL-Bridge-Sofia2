<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <Console name="Debug" target="SYSTEM_OUT">
            <PatternLayout pattern="[%t] %-5level %l - %msg%n" />
        </Console>
    </Appenders>
    <Loggers>
        <Root level="error">
            <AppenderRef ref="Debug" />
        </Root>
        <Logger name="eu.interiot.intermw.bridge.example" level="debug"/>
    </Loggers>
</Configuration>